<!-- Loop over each object in the games array -->
{{#each games as |game| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      <img src="{{game.img_url}}" alt="{{game.title}}">


    </h2>
  </div>
</div>
{{/each}}
